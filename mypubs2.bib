	keywords = {WESTERN UNITED-STATES; ON-SNOW; PRECIPITATION PHASE; PACIFIC-NORTHWEST; MOISTURE; SURFACE; EVENTS; VARIABILITY; VEGETATION; DENSITY},
	number = {1},
