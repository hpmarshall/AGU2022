	doi = {10.1016/j.coldregions.2009.04.003},
	issn = {0165-232X},
	journal = {Cold Regions Science and Technology},
	keywords = {SNOW},
	month = {November},
	number = {2-3},
	pages = {202--209},
	publisher = {ELSEVIER SCIENCE BV},
	times-cited = {7},
	title = {Quantifying changes in weak layer microstructure associated with artificial load changes},
	volume = {59},
	wok-uid = {WOS:000272059600013},
	year = {2009},
