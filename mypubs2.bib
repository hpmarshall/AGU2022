	keywords = {TERRAIN-BASED PARAMETERS; LANDSAT THEMATIC MAPPER; WATER EQUIVALENT; SPATIAL-DISTRIBUTION; GRAIN-SIZE; MODIS; CATCHMENT; BALANCE; MODEL; AREA},
	month = {September},
	pages = {413--425},
