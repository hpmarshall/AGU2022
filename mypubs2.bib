	keywords = {GREENLAND ICE-SHEET; GROUND-PENETRATING RADAR; LONG-TERM SNOW; WET-SNOW; FIRN; COVER; TEMPERATURE; RETENTION; ABLATION; BALANCE},
	month = {October},
