	keywords = {SPATIAL VARIABILITY; MOUNTAIN CATCHMENT; WATER EQUIVALENT; ALPINE CATCHMENT; ACCUMULATION; FOREST; SCALE; DEPTH; MODEL; PATTERNS},
	month = {June},
	number = {6},
