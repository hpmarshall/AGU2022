	issn = {0022-1430},
	journal = {Journal of Glaciology},
	keywords = {GROUND-PENETRATING RADAR; TAYLOR-DOME; SNOW ACCUMULATION; INTERNAL LAYERS; WEST ANTARCTICA; EAST ANTARCTICA; TEMPORAL VARIABILITY; STABLE-ISOTOPES; ROSS EMBAYMENT; SIPLE DOME},
	number = {183},
	pages = {694--712},
	publisher = {INT GLACIOL SOC},
	times-cited = {43},
	title = {Inference of accumulation-rate patterns from deep layers in glaciers and ice sheets},
	volume = {53},
