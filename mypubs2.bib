	journal = {Journal of Geophysical Research-Earth Surface},
	keywords = {HARDNESS; ICE},
	month = {November},
	number = {F04016},
	publisher = {AMER GEOPHYSICAL UNION},
	times-cited = {22},
	title = {Accurate inversion of high-resolution snow penetrometer signals for microstructural and micromechanical properties},
