	journal = {Remote Sensing of Environment},
	keywords = {MOUNTAIN CATCHMENT; WATER EQUIVALENT; UNITED-STATES; TERRAIN; CLIMATE; CLASSIFICATION; ACCUMULATION; VARIABILITY; SUCCESSION; DEPOSITION},
	month = {February},
